{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-unused-variable": true,
    "interface-over-type-literal": false,
    "no-shadowed-variable": false,
    "no-unused-expression": true,
    "member-ordering": false,
    "no-empty": false,
    "variable-name": [true, "ban-keywords", "allow-leading-underscore", "allow-pascal-case"],
    "space-before-function-paren": [true, { "anonymous": "always", "named": "always", "asyncArrow": "always", "method": "never", "constructor": "never" }],
    "quotemark": [true, "single", "avoid-template"],
    "prefer-function-over-method": true,
    "object-literal-shorthand": true,
    "object-literal-key-quotes": [true, "as-needed"],
    "no-unnecessary-initializer": true,
    "one-line": [true, "check-catch", "check-finally", "check-else", "check-open-brace", "check-whitespace"],
    "arrow-return-shorthand": true,
    "only-arrow-functions": true,
    "no-unnecessary-callback-wrapper": true,
    "class-name": true,
    "no-boolean-literal-compare": true,
    "no-restricted-globals": [true, "name", "length", "event"],
    "align": [true, "elements", "members", "parameters", "statements"],
    "no-irregular-whitespace": true,
    "no-trailing-whitespace": true,
    "whitespace": [true, "check-branch", "check-decl", "check-operator", "check-module", "check-separator", "check-rest-spread", "check-type", "check-typecast", "check-type-operator", "check-preblock"],
    "eofline": true,
  },
  "rulesDirectory": []
}
