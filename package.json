{
  "name": "react-boilerplate",
  "version": "1.2.0",
  "description": "a small react-redux boilerplate app",
  "scripts": {
    "dev": "webpack-dev-server --devtool eval --progress",
    "lint": "eslint ./app/",
    "test": "babel test/ -d test-dist/ && node test-dist/ | tap-summary",
    "build": "NODE_ENV=production webpack --progress",
    "validate": "npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jameslaneconkling/react-boiler.git"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "es6",
    "react",
    "redux"
  ],
  "author": "jameslaneconkling",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.18.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^3.11.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "tape": "^4.6.3",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-validator": "^2.2.9"
  },
  "dependencies": {
    "ramda": "^0.22.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.6",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0"
  },
  "bugs": {
    "url": "https://github.com/jameslaneconkling/react-boiler/issues"
  },
  "homepage": "https://github.com/jameslaneconkling/react-boiler#readme",
  "main": "dist/index.html"
}
