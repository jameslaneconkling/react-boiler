{
  "name": "react-boilerplate",
  "version": "1.2.0",
  "description": "a small react-redux boilerplate app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jameslaneconkling/react-boiler.git"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "es6",
    "react",
    "redux"
  ],
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.dev.js --devtool eval --progress",
    "lint": "eslint ./app/",
    "test": "babel test/ -d test-dist/ && node test-dist/ | tap-summary",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js --progress",
    "validate": "npm run lint && npm run test && npm run build"
  },
  "author": "jameslaneconkling",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.18.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.1",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "tape": "^4.6.3",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "ramda": "^0.23.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "recompose": "^0.23.4",
    "redux": "^3.6.0",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.4.0"
  },
  "babel": {
    "plugins": ["transform-object-rest-spread", "babel-plugin-transform-class-properties"],
    "presets": ["es2015", "react"]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "rules": {
      "comma-dangle": ["error", "never"],
      "no-multi-spaces": 0,
      "no-confusing-arrow": ["error", { "allowParens": true }],
      "max-len": [2, { "code": 100, "ignoreComments": true, "ignoreTrailingComments": true, "ignoreTemplateLiterals": true }],
      "import/prefer-default-export": 0,
      "no-console": 0
    }
  },
  "main": "dist/index.html"
}
