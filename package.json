{
  "name": "react-boilerplate",
  "version": "1.3.0",
  "description": "a small react-redux boilerplate app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jameslaneconkling/react-boiler.git"
  },
  "keywords": [
    "boilerplate",
    "es6",
    "react",
    "redux",
    "webpack"
  ],
  "scripts": {
    "dev": "webpack-dev-server --mode development --config webpack.dev.js --hotOnly --progress",
    "lint": "eslint ./src --ext .js --ext .jsx",
    "test": "node test/",
    "build": "webpack --mode production --config webpack.prod.js --progress",
    "validate": "npm run test && npm run lint && npm run build"
  },
  "author": "James Conkling <james.lane.conkling@gmail.com> jameslaneconkling.github.io",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "tape": "^4.8.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.5.6"
  },
  "main": "dist/index.html"
}
